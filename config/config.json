{
    "classes": [
        { "positive": ["HCC"], "negative": ["F4"] }
    ],
    "encoding": ["one-hot", "recessive"],
    "feature_selection": [{ "method": "pca", "k": 3 }, { "method": "k-best", "k": 2 }],
    "test_size": [0.2, 0.15],
    "classifiers": [
        {
            "clf": "rfc",
            "param_grid": {
                "n_estimators": [100, 150, 200],
                "max_features": ["auto", "sqrt", "log2"],
                "max_depth": [6, 7, 8],
                "criterion": ["gini", "entropy"]
            },
            "cv": 5
        },
        {
            "clf": "svc",
            "param_grid": {
                "C": [0.001, 0.01, 0.1, 1, 10],
                "gamma": [0.001, 0.01, 0.1, 1],
                "kernel": ["rbf", "linear", "poly"]
            },
            "cv": 5
        }
    ]
}